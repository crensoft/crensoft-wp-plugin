{
  "name": "@crensoft/wp-plugin",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "clean-dist": "rm -rf ./dist/editor.js && rm -rf ./dist/style.js && rm -rf ./dist/css.js",
    "build": "node scripts/build.js && yarn clean-dist",
    "storybook": "start-storybook -s ./public -p 9001"
  },
  "dependencies": {
    "@crensoft/mui-core": "^0.1.0",
    "@crensoft/mui-marketing": "^0.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/classnames": "^2.2.7",
    "@types/color": "^3.0.0",
    "@types/jquery": "^3.3.29",
    "@types/node": "^11.13.9",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-loadable": "^5.5.1",
    "@types/react-text-mask": "^5.4.4",
    "autoprefixer": "^9.5.1",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.21.1",
    "cgb-dev-utils": "^1.5.0",
    "chalk": "^2.3.0",
    "classnames": "^2.2.6",
    "color": "^3.1.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "1.0.1",
    "filesize": "^4.1.2",
    "fork-ts-checker-webpack-plugin": "^1.3.6",
    "formik": "^1.5.4",
    "fs-extra": "^7.0.1",
    "gzip-size": "^5.1.0",
    "inquirer": "^6.3.1",
    "jss": "^10.0.0-alpha.16",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.4.0",
    "postcss-loader": "^3.0.0",
    "pretty-bytes": "^5.2.0",
    "raw-loader": "^2.0.0",
    "react-dropzone": "^10.1.4",
    "react-loadable": "^5.5.0",
    "react-scroll-up": "^1.3.3",
    "react-text-mask": "^5.4.3",
    "resolve-pkg": "^2.0.0",
    "rxjs": "^6.5.1",
    "sass-loader": "^7.1.0",
    "shelljs": "^0.8.0",
    "shortid": "^2.2.14",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "update-notifier": "^2.3.0",
    "webpack": "^4.3",
    "webpack-merge": "^4.2.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 Chrome versions",
              "last 2 Firefox versions",
              "last 2 Safari versions",
              "last 2 iOS versions",
              "last 1 Android version",
              "last 1 ChromeAndroid version",
              "ie 11"
            ]
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ]
    ]
  },
  "devDependencies": {
    "@crensoft/mui-utils": "0.1.1",
    "@material-ui/core": "^4.1.0",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/storybook__addon-a11y": "^5.0.0",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react": "^4.0.1",
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-wordpress": "^0.1.0",
    "jquery": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "peerDependencies": {
    "@crensoft/mui-utils": "0.1.1",
    "@material-ui/core": "^4.1.0",
    "@material-ui/styles": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "sideEffects": [
    "*.block.tsx",
    "block.tsx",
    "*.scss",
    "*.css"
  ]
}
